            {{from gluon.serializers import json}}
            var suma = {{=XML(json(y))}};
var sum = {{=XML(json(x))}};
    </script>

        <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.6.1.min.js"></script>
        <script type="text/javascript">
            var graph;
            var xPadding = 20;
            var yPadding = 30;
                    var k=sum.length;
    var values=[];
    var val1=[];
            for (var i=0;i<k;i++)
           {
               //if (sum[i]!='undefined')
                  //  value.push({ X:"c"},{Y:suma[i]});
               
                    values[i]=suma[i];
                   val1[i]=sum[i];
                    //document.write(sum[i]);
               
         }
         //   var data={values:value};

            // Returns the max Y value in our data list
            function getMaxY() {
                var max = 0;
                
                for(var i = 0; i < values.length; i ++) {
                    if(values[i] > max) {
                        max = values[i];
                    }
                }
                
                max += 10 - max % 10;
                return max;
            }
            
            // Return the x pixel for a graph point
            function getXPixel(val) {
                return ((graph.width() - xPadding) / values.length) * val + (xPadding * 1.5);
            }
            
            // Return the y pixel for a graph point
            function getYPixel(val) {
                return graph.height() - (((graph.height() - yPadding) / getMaxY()) * val) - yPadding;
            }

            $(document).ready(function() {
                graph = $('#graph');
                var c = graph[0].getContext('2d');            
                
                c.lineWidth = 2;
                c.strokeStyle = '#333';
                c.font = 'italic 8pt sans-serif';
                c.textAlign = "center";
                
                // Draw the axises
                c.beginPath();
                c.moveTo(xPadding, 0);
                c.lineTo(xPadding, graph.height() - yPadding);
                c.lineTo(graph.width(), graph.height() - yPadding);
                c.stroke();
                
                // Draw the X value texts
                for(var i = 0; i < values.length; i ++) {
                    c.fillText(val1[i], getXPixel(i), graph.height() - yPadding + 20);
                }
                
                // Draw the Y value texts
                c.textAlign = "right"
                c.textBaseline = "middle";
                
                for(var i = 0; i < getMaxY(); i += 10) {
                    c.fillText(i, xPadding - 10, getYPixel(i));
                }
                
                c.strokeStyle = '#f00';
                
                // Draw the line graph
                c.beginPath();
                c.moveTo(getXPixel(0), getYPixel(values[0]));
                for(var i = 1; i < values.length; i ++) {
                    c.lineTo(getXPixel(i), getYPixel(values[i]));
                }
                c.stroke();
                
                // Draw the dots
                c.fillStyle = '#333';
                
                for(var i = 0; i < values.length; i ++) {  
                    c.beginPath();
                    c.arc(getXPixel(i), getYPixel(values[i]), 4, 0, Math.PI * 2, true);
                    c.fill();
                }
